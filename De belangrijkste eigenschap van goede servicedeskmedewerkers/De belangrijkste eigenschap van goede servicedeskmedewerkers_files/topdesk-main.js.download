$(document).ready(function (){
        
    /* 
        Vreselijk ;=)
        Below is for the Custom Module: TOP Row Hider
        - Adds css class 'hidden', to hide a row. 
        The custom module 'TOP Row Hider' can be used (multiple times) in a template like the TOP Webinar Overview Template.
        This module inserts an html element when the checkbox is true with id = topRowHider.
        The row in the template also need the css class topRow Hider.
        Using this two-step chaining ( .parents and .closest ) because there is a lot of row-nesting going on in some templates.
        - TD
    */
    $("input[id='topRowHider']").parents(".topRowHider").closest(".row-fluid-wrapper").addClass("hidden");
        
    // MC-2432
    // Because Hubspot only provides the option for one URL
    var pathname = window.location.pathname; 
    
    if(pathname == "/nl"){
        $("#twitter").attr("href","https://web.archive.org/web/20210901103213/https://twitter.com/TOPdesk_NL");
        $("#facebook").attr("href","https://web.archive.org/web/20210901103213/https://www.facebook.com/TOPdeskNL/");
    } else if (pathname == "/de") {
        $("#twitter").attr("href","https://web.archive.org/web/20210901103213/https://twitter.com/TOPdesk_DE");
    }     
    
});


function getCountry(){
    if ($('#countryKey').length){
        var office =  $('#countryKey').val();
        return (office)? office : "uk";
    } else {
        var url = window.location.href;
        url = url.split("/");
        return url[Constants.countryUrlPosition];
    }
}

function changeURL (object, url){
    object.attr('href', url);
}

function addCSS(filename){
 var head = document.getElementsByTagName('head')[0];

 var css = document.createElement('link');
 css.href = filename;
 css.type = 'text/css';
 css.rel = 'stylesheet';

 head.append(css);
}

